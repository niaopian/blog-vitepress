{
  "name": "vitepress-plugin-group-icons",
  "type": "module",
  "version": "1.6.3",
  "packageManager": "pnpm@10.12.4",
  "author": "Leo <https://github.com/yuyinws>",
  "license": "MIT",
  "homepage": "https://github.com/yuyinws/vitepress-plugin-group-icons#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuyinws/vitepress-plugin-group-icons.git"
  },
  "bugs": "https://github.com/yuyinws/vitepress-plugin-group-icons/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "markdown-it": ">=14",
    "vite": ">=3"
  },
  "dependencies": {
    "@iconify-json/logos": "^1.2.4",
    "@iconify-json/vscode-icons": "^1.2.29",
    "@iconify/utils": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.1",
    "@antfu/ni": "^25.0.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.15.34",
    "bumpp": "^10.2.0",
    "eslint": "^9.30.0",
    "esno": "^4.8.0",
    "lint-staged": "^16.1.2",
    "simple-git-hooks": "^2.13.0",
    "tsdown": "^0.13.4",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vitepress": "^1.6.3",
    "vitest": "^3.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
